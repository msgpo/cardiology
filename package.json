{
  "name": "cardiology",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "git@github.com:stanford-oval/cardiology.git",
  "contributors": [
    {
      "name": "Euirim Choi",
      "email": "euirim@stanford.edu"
    },
    {
      "name": "Swee Kiat Lim",
      "email": "sweekiat@stanford.edu"
    }
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --noEmit",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "check-types": "tsc --noEmit",
    "check-types:watch": "npm run check-types -- --watch",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "start-dev": "nodemon --config \"./nodemon.json\"/",
    "start-prod": "node -r dotenv/config build/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.17",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.0-beta.8",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
